<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AudioWaveView">
        <attr name="vertical_half_offset" format="integer"/>
        <attr name="horizontal_half_offset" format="integer"/>
        <attr name="wave_count_per_pixel" format="float"/>
    </declare-styleable>
    <declare-styleable name="UserItemView">
        <attr name="left_icon" format="reference"/>
        <attr name="show_left_icon" format="boolean"/>
        <attr name="left_text" format="string"/>
        <attr name="right_text" format="string"/>
        <attr name="show_right_arrow" format="boolean"/>
        <attr name="show_bottom_line" format="boolean"/>
    </declare-styleable>
</resources>